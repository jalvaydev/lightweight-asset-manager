type Asset {
  id: ID!
  name: String!
  description: String!
  cost: Int!
}

type User {
  id: ID!
  name: String!
  access: String!
}

type Query {
  assets: [Asset!]!
}

input NewAsset {
  name: String!
  description: String!
  cost: Int!
}

type Mutation {
  createAsset(input: NewAsset!): Asset!
}
